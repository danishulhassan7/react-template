


<!DOCTYPE html>
<html lang="en">

<!-- Mirrored from www.konnectplugins.com/edu-course/apply-course.html by HTTrack Website Copier/3.x [XR&CO'2014], Sun, 22 Mar 2020 15:49:18 GMT -->
<head>
    @{

        
        ViewBag.Title = "contactform";
    }

</head>

<!-- Body -->
<body>
    <!-- Registrations page-->
    <div ng-controller="headerCtrl">
        <div class="container" style="background-color:white ; width:80% ">
            <h2 style="color:black; text-align:center"><b>Contact Form</b></h2>
            <div class="panel-group" id="accordion">
                <div class="panel panel-default">
                    <div class="panel-body">
                        <section>
                            <div class="row">
                                <div class="col-md-10 offset-md-1">
                                    <div class="form-horizontal">

                                        <div class="form-group">
                                            @Html.Label("Contact NO", new { @class = "control-label col-md-6" })
                                            <div class="col-md-12  col-sm-12">
                                                @Html.TextBox("contact", null, new { @class = "form-control   ", @placeholder = "Enter Contact No" })
                                            </div>
                                        </div>


                                        <div class="form-group">
                                            @Html.Label("Description", new { @class = "control-label col-md-6" })
                                            <div class="col-md-12  col-sm-12">
                                                @Html.TextArea("name", null, new { @class = "form-control   ", @placeholder = "Enter Description" })
                                            </div>


                                        </div>

                                        <div class="form-group ">
                                            <div class="col-md-4">
                                                <button type="submit" class="btn btn btn-success"> ADD </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </section>
                    </div>
                </div>
            </div>


            <script src="~/Scripts/jquery-3.3.1.min.js"></script>
            <script src="~/Scripts/jquery.validate.min.js"></script>
            <script src="~/Scripts/jquery.validate.unobtrusive.js"></script>

            @if (ViewBag.Message != null)
            {
                <script type="text/javascript">

         window.onload = function () {
          alert("@ViewBag.Message");
          };
                </script>

            }


</body>

