<head>

    @{
        ViewBag.Title = "Enrollment";
    }
    <link href="~/Content/themes/base/jquery-ui.min.css" rel="stylesheet" />

</head>


<!-- Registrations page-->
<div ng-controller="headerCtrl">
    <div class="container" style="background-color:white ; width:80% ">
        <h2 style="color:black; text-align:center"><b>Detail Form</b></h2>
        <div class="panel-group" id="accordion">
            <div class="panel panel-default">
                <div class="panel-body">
                    <section>
                        <div class="row">
                            <div class="col-md-10 offset-md-1">
                                @using (Html.BeginForm())
                                {
                                    @Html.AntiForgeryToken()
                                    <div class="form-horizontal">
                                        <div class="form-group">
                                            @Html.Label("Registered Complain DateTime", new { @class = "control-label col-md-6" })
                                            <div class="col-md-12  col-sm-12">
                                                @Html.TextBox("Date", null, new { @class = "form-control  ", @readonly = "readonly" })
                                            </div>


                                        </div>


                                        <div class="form-group">
                                            @Html.Label("Student Name", new { @class = "control-label col-md-6" })
                                            <div class="col-md-12  col-sm-12">
                                                @Html.TextBox("name", null, new { @class = "form-control  ", @readonly = "readonly" })
                                            </div>


                                        </div>

                                        <div class="form-group">
                                            @Html.Label("Complain Detail", new { @class = "control-label col-md-6" })
                                            <div class="col-md-12  col-sm-12">
                                                @Html.TextAreaFor(model => model.complain_description, 5, 40, htmlAttributes: new { @class = "form-control ", style = "width: 100%; max-width: 100%;", @readonly = "readonly" })
                                            </div>


                                        </div>
                                        <div class="form-group">
                                            @Html.Label("Reply", new { @class = "control-label col-md-6" })
                                            <div class="col-md-12  col-sm-12">

                                                @Html.TextAreaFor(model => model.replymsg, 5, 40, htmlAttributes: new { @class = "form-control ", style = "width: 100%; max-width: 100%;" })
                                                @Html.ValidationMessageFor(x => Model.replymsg, "", new { @class = "text-danger" })


                                            </div>


                                        </div>
                                        @*<div class="form-group">
                                                    @Html.Label("Class", new { @class = "control-label col-md-6" })
                                                    <div class="col-md-12  col-sm-12">
                                                        @Html.TextBox("Class", null, new { @class = "form-control   ", @readonly = "readonly" })


                                                    </div>

                                                </div>

                                            </div>*@


                                        @*<div class="form-group">
                                                @Html.Label("Course", new { @class = "control-label col-md-6" })

                                                <div class="col-md-12  col-sm-12">
                                                    @Html.DropDownList("Select Course", (IEnumerable<SelectListItem>)ViewData["Courses"], "Select Course", new { @class = "form-control" })
                                                    @Html.ValidationMessageFor(x => Model.CourseId, "", new { @class = "text-danger" })

                                                </div>

                                            </div>*@



                                        <div class="form-group ">
                                            <div class="col-md-4">
                                                <button type="submit" class="btn btn btn-success"> ADD </button>
                                            </div>
                                        </div>
                                    </div>
                                }
                            </div>
                    </section>
                </div>
            </div>
        </div>


        <script src="~/Scripts/jquery-3.3.1.min.js"></script>
        <script src="~/Scripts/jquery.validate.min.js"></script>
        <script src="~/Scripts/jquery.validate.unobtrusive.js"></script>




