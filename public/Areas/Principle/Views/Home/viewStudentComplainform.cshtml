
@model IEnumerable<edu_course.Models.tblStudentComplainValidation>
<!DOCTYPE html>
<html lang="en">

<head>
    @{
       ViewBag.Title = "viewComplainform";
            Layout = "~/Areas/Principle/Views/Shared/_Layout.cshtml";

    }
</head>

<body id="page-top">

    <!-- Page Wrapper -->
    <div id="wrapper">

        <!-- End of Topbar -->
        <!-- Begin Page Content -->
        <div class="container-fluid">

            <!-- Page Heading -->
            <!-- DataTales Example -->
            <div class="card shadow mb-4">
                <div class="card-header py-3">
                    <h6 class="m-0 font-weight-bold text-primary">Student Complain</h6>
                    @*<button type="button" class="btn btn-primary">PDF</button>*@
                </div>
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                            <thead>
                                <tr>
                                    <th>Student Name</th>
                                    <th>School Naem</th>
                                    <th>Description</th>
                                    <th>Date</th>
                                    <th></th>

                                </tr>
                            </thead>
                            @*<tfoot>
                                    <tr>
                                        <th>Course Code</th>
                                        <th>Name</th>
                                        <th>Action Generate</th>
                                    </tr>
                                </tfoot>*@
                            <tbody>
                                @foreach (var item in Model)
                                {
                                    <tr>
                                        <td>
                                            @item.StudentName
                                        </td>
                                        <td>
                                            @item.SchoolName
                                        </td>
                                        <td>
                                            @item.complain_description
                                        </td>

                                        <td>
                                            @item.complain_date
                                        </td>

                                        <td>
                                            <button class="button button3" style="background-color: #5DC560; border-color:#5DC560; border-radius:5%;">@Html.ActionLink("Student Complain Reply", "viewStudentComplainReply", new { id = item.complain_id }, new { Style = "color:White; text-decoration:none;" })</button>
                                        </td>

                                    </tr>

                                }



                            </tbody>
                        </table>
                    </div>
                </div>
            </div>

        </div>
        <!-- /.container-fluid -->

    </div>
    <!-- End of Main Content -->
    <!-- Footer -->
    <footer class="sticky-footer bg-white">
        <div class="container my-auto">
            <div class="copyright text-center my-auto">
                <span>Copyright &copy; Your Website 2019</span>
            </div>
        </div>
    </footer>
    <!-- End of Footer -->
    </div>
    <!-- End of Content Wrapper -->
    </div>
    <!-- End of Page Wrapper -->
    <!-- Scroll to Top Button-->
    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fas fa-angle-up"></i>
    </a>

    <!-- Logout Modal-->
    <div class="modal fade" id="logoutModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">Ready to Leave?</h5>
                    <button class="close" type="button" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">×</span>
                    </button>
                </div>
                <div class="modal-body">Select "Logout" below if you are ready to end your current session.</div>
                <div class="modal-footer">
                    <button class="btn btn-secondary" type="button" data-dismiss="modal">Cancel</button>
                    <a class="btn btn-primary" href="login.html">Logout</a>
                </div>
            </div>
        </div>
    </div>
</body>
</html>




